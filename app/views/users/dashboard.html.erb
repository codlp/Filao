<%= render 'shared/sidebar' %>
<div class="all-without-sidebar">
  <div class="navnav">
    <%= render 'shared/navbar' %>
  </div>
  <div class="main-screen">
    <div class="presentation-line">
      <h1>My Dashboard</h1>
    </div>
    <div class="grid-container">
      <div id="calendar">
        <%= @google_events %>

    </div>

        <%#= month_calendar do |date| %>
        <%#= date %>
        <%# end %>
      </div>

    </div>
  <div class="in-process">
      <h3>Tasks in process</h3>
        <h2 class="count-tasks"> <%= @tasks.count %>  </h2>
        <div class="call-btn">
          <%= link_to "See all tasks", projects_path(current_user), class: 'call-to-action' %>
      </div>
  </div>
</div>
